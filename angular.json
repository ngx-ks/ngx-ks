{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "defaultProject": "kickstart-demo",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "kickstart-demo": {
      "projectType": "application",
      "root": "apps/kickstart-demo",
      "sourceRoot": "apps/kickstart-demo/src",
      "prefix": "ngx-ks",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/kickstart-demo",
            "index": "apps/kickstart-demo/src/index.html",
            "main": "apps/kickstart-demo/src/main.ts",
            "polyfills": "apps/kickstart-demo/src/polyfills.ts",
            "tsConfig": "apps/kickstart-demo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/kickstart-demo/src/favicon.ico",
              "apps/kickstart-demo/src/assets"
            ],
            "styles": ["./node_modules/@angular/material/prebuilt-themes/indigo-pink.css", "apps/kickstart-demo/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/kickstart-demo/src/environments/environment.ts",
                  "with": "apps/kickstart-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kickstart-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "kickstart-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kickstart-demo:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/kickstart-demo/src/**/*.ts",
              "apps/kickstart-demo/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/kickstart-demo"],
          "options": {
            "jestConfig": "apps/kickstart-demo/jest.config.js",
            "passWithNoTests": true
          }
        },
        "release": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "echo 'Nothing to release here...'"
              }
            ]
          }
        }
      }
    },
    "kickstart-demo-e2e": {
      "root": "apps/kickstart-demo-e2e",
      "sourceRoot": "apps/kickstart-demo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/kickstart-demo-e2e/cypress.json",
            "tsConfig": "apps/kickstart-demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "kickstart-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kickstart-demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/kickstart-demo-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "component-state": {
      "projectType": "library",
      "root": "libs/component-state",
      "sourceRoot": "libs/component-state/src",
      "prefix": "ngx-ks",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/component-state/tsconfig.lib.json",
            "project": "libs/component-state/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/component-state/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/component-state/src/**/*.ts",
              "libs/component-state/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/component-state"],
          "options": {
            "jestConfig": "libs/component-state/jest.config.js",
            "passWithNoTests": true
          }
        },
        "release": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn semantic-release --debug --extends=./libs/component-state/release.config.js"
              }
            ]
          }
        }
      }
    },
    "ui-container": {
      "projectType": "library",
      "root": "libs/ui-container",
      "sourceRoot": "libs/ui-container/src",
      "prefix": "ngx-ks",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ui-container/tsconfig.lib.json",
            "project": "libs/ui-container/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ui-container/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-container/src/**/*.ts",
              "libs/ui-container/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui-container"],
          "options": {
            "jestConfig": "libs/ui-container/jest.config.js",
            "passWithNoTests": true
          }
        },
        "release": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn semantic-release --debug --extends=./libs/ui-container/release.config.js"
              }
            ]
          }
        }
      }
    },
    "animations": {
      "projectType": "library",
      "root": "libs/animations",
      "sourceRoot": "libs/animations/src",
      "prefix": "ngx-ks",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/animations/tsconfig.lib.json",
            "project": "libs/animations/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/animations/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/animations/src/**/*.ts",
              "libs/animations/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/animations"],
          "options": {
            "jestConfig": "libs/animations/jest.config.js",
            "passWithNoTests": true
          }
        },
        "release": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn semantic-release --debug --extends=./libs/animations/release.config.js"
              }
            ]
          }
        }
      }
    }
  }
}
